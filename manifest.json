{
"manifest_version": 1,
    "name": "NickNamer",
    "description": "Make a nickname for anyone you know",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}
