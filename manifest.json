{
"manifest_version": 1,
    "name": "NickNamer",
    "description": "Make a nickname for anyone you know",
    "version": "1.0",
    "browser_action": {
      "default_title": "NickNamer",
      "default_icon": "nickname_icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
                "background.js"
            ],
            "run_at": "document_end"
        },
    ]
}
